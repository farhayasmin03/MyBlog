<!doctype html>
<html lang="en">

<head>
  <meta charset="utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
  <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.2.1/css/bootstrap.min.css" integrity="sha384-GJzZqFGwb1QTTN6wy59ffF1BuGJpLSa9DkKMp0DgiMDm4iYMj70gZWKYbI706tWS"
    crossorigin="anonymous" />
  <link rel="stylesheet" href="css/app.css" />
  <title>Farhana Yasmin's Blog</title>

</head>

<body>
  <div class="container">
    <header>
      <h1><a href="/">Farhana Yasmin</a></h1>
    </header>
    <div class="row">
      <div class="col-8">
        <div class="post-card">

          <h2><a href="#">
              <div class="list-container">

                <% blog.forEach( function( blog ){ %>
                <div class="list-box">
                  <P style="color:rgb(108, 106, 235)"><%= moment().format('Do MMMM, YYYY'); %></P>
                  <P style="color:rgb(15, 14, 14)"><%= blog.text %></P>
                </div>
                <% }); %>
              </div>
            </a></h2>
          
        </div>
        


      </div>
      <div class="col-4">
        <div class="post-card">
          <ul class="list-unstyled">
            <li><a href="/new_post">Create a new post</a></li>
            <li><a href="/signUp">Sign up</a></li>
            <li><a href="/login">Login</a></li>
          </ul>
        </div>
      </div>
    </div>
  </div>
</body>

</html>